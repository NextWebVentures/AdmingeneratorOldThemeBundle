{% block list_actions %}
{{ echo_block("list_actions") }}
    {% if object_actions|length > 0 %}
     <ul class="actions">
        {% for action in builder.Actions %}
            <li><a class="{{ action.name }}" href="{{ echo_path((action.route ? action.route : bundle_name ~ '_' ~ action.name)) }}">{{ action.label }}</a></li>
        {% endfor %}
     </ul>
    {% endif %}
{{ echo_endblock() }}
{% endblock %}